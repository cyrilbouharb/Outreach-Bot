{
  "name": "outreach-bot",
  "version": "1.0.0",
  "description": "This guide will walk you through setting up the database for the Outreach Bot project locally.",
  "main": "index.js",
  "scripts": {
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "start-client": "cd client && npm start",
    "start-server": "cd server && npm start",
    "build": "npm run install-client && npm run install-server",
    "start": "concurrently --kill-others \"npm run start-server\" \"npm run start-client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/V1d17/Outreach-Bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/V1d17/Outreach-Bot/issues"
  },
  "homepage": "https://github.com/V1d17/Outreach-Bot#readme",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
